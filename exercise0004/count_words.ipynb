    "import re\n",
    "def count_words(file):\n",
    "    file = open(file, 'rt')\n",
    "    file_text = file.read()\n",
    "    word = re.findall(r'[a-zA-Z0-9]+' ,file_text)\n",
    "    file.close()\n",
    "    print(len(word))\n",
    "    \n",
    "if __name__ == '__main__':\n",
    "    count_words(\"Article.txt\")"
   ]
  }
